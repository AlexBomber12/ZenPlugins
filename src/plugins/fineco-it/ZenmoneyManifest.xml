<?xml version="1.0" encoding="utf-8"?>
<plugin>
  <!-- УНИКАЛЬНЫЙ идентификатор плагина -->
  <name>fineco-it</name>

  <!-- Версия плагина. При каждом релизе ↑ -->
  <version>1.0.0</version>

  <!-- Как увидит пользователь в списке банков -->
  <title>Fineco Bank Italy</title>

  <!-- Короткое описание (до 200 симв.) -->
  <description>Импорт счетов и операций из Fineco Bank (PSD2 API).</description>

  <!-- Минимальная версия SDK; 1 — совместима со stable Zenmoney -->
  <minSdkVersion>1</minSdkVersion>

  <!-- Указывает на СКОМПИЛИРОВАННЫЙ JS (после build) -->
  <mainFile>index.js</mainFile>

  <!-- XML с полями авторизации -->
  <preferences>preferences.xml</preferences>
</plugin>
